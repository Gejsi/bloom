{
  "name": "fenrir",
  "version": "1.0.0",
  "description": "A transpiler that simplifies the development of serverless functions.",
  "keywords": [
    "serverless",
    "transpiler"
  ],
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "fen": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --watch",
    "dev": "node dist/index.js"
  },
  "author": "Gejsi <vgejsi@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "ansi-colors": "^4.1.3",
    "cleye": "^1.3.2"
  },
  "peerDependencies": {
    "fenrir-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsconfig": "workspace:*",
    "tsup": "^6.5.0"
  },
  "tsup": {
    "format": "esm",
    "sourcemap": true,
    "clean": true,
    "minify": true
  }
}
